{
  "name": "@adonisjs/http-server",
  "version": "8.0.0",
  "description": "AdonisJS HTTP server with support packed with Routing and Cookies",
  "main": "build/index.js",
  "type": "module",
  "files": [
    "build",
    "!build/bin",
    "!build/tests"
  ],
  "exports": {
    ".": "./build/index.js",
    "./helpers": "./build/src/helpers.js",
    "./types": "./build/src/types/main.js",
    "./client/url_builder": "./build/src/client/url_builder.js",
    "./factories": "./build/factories/main.js"
  },
  "engines": {
    "node": ">=24.0.0"
  },
  "scripts": {
    "pretest": "npm run lint",
    "test": "cross-env NODE_DEBUG=adonisjs:http npm run quick:test",
    "typecheck": "tsc --noEmit",
    "precompile": "npm run lint",
    "compile": "tsdown && tsc --emitDeclarationOnly --declaration",
    "build": "npm run compile",
    "prebenchmark": "npm run build",
    "benchmark": "node benchmarks/index.js",
    "release": "release-it",
    "version": "npm run build",
    "format": "prettier --write .",
    "prepublishOnly": "npm run build",
    "lint": "eslint",
    "quick:test": "node --import=@poppinss/ts-exec --enable-source-maps bin/test.ts",
    "docs": "typedoc"
  },
  "keywords": [
    "http",
    "http-server"
  ],
  "author": "virk,adonisjs",
  "license": "MIT",
  "devDependencies": {
    "@adonisjs/application": "^9.0.0",
    "@adonisjs/eslint-config": "^3.0.0",
    "@adonisjs/events": "^10.1.0",
    "@adonisjs/fold": "^11.0.0",
    "@adonisjs/logger": "^7.1.0",
    "@adonisjs/prettier-config": "^1.4.5",
    "@adonisjs/tsconfig": "^2.0.0",
    "@boringnode/encryption": "^1.0.0",
    "@fastify/middie": "^9.1.0",
    "@japa/assert": "^4.2.0",
    "@japa/expect-type": "^2.0.4",
    "@japa/file-system": "^3.0.0",
    "@japa/runner": "^5.3.0",
    "@japa/snapshot": "^2.0.10",
    "@poppinss/ts-exec": "^1.4.4",
    "@release-it/conventional-changelog": "^10.0.5",
    "@types/accepts": "^1.3.7",
    "@types/content-disposition": "^0.5.9",
    "@types/destroy": "^1.0.3",
    "@types/encodeurl": "^1.0.3",
    "@types/etag": "^1.8.4",
    "@types/fresh": "^0.5.3",
    "@types/fs-extra": "^11.0.4",
    "@types/mime-types": "^3.0.1",
    "@types/node": "^25.3.0",
    "@types/on-finished": "^2.3.5",
    "@types/pem": "^1.14.4",
    "@types/proxy-addr": "^2.0.3",
    "@types/supertest": "^6.0.3",
    "@types/type-is": "^1.6.7",
    "@types/vary": "^1.1.3",
    "@vinejs/vine": "^4.3.0",
    "accepts": "^1.3.8",
    "autocannon": "^8.0.0",
    "c8": "^11.0.0",
    "cross-env": "^10.1.0",
    "eslint": "^10.0.2",
    "fastify": "^5.7.4",
    "fs-extra": "^11.3.3",
    "get-port": "^7.1.0",
    "http-status-codes": "^2.3.0",
    "pem": "^1.14.8",
    "prettier": "^3.8.1",
    "reflect-metadata": "^0.2.2",
    "release-it": "^19.2.4",
    "supertest": "^7.2.2",
    "tsdown": "^0.20.3",
    "typedoc": "^0.28.17",
    "typescript": "^5.9.3",
    "youch": "^4.1.0"
  },
  "dependencies": {
    "@poppinss/macroable": "^1.1.0",
    "@poppinss/matchit": "^3.2.0",
    "@poppinss/middleware": "^3.2.7",
    "@poppinss/qs": "^6.15.0",
    "@poppinss/utils": "^7.0.0",
    "@sindresorhus/is": "^7.2.0",
    "content-disposition": "^1.0.1",
    "cookie-es": "^2.0.0",
    "destroy": "^1.2.0",
    "encodeurl": "^2.0.0",
    "etag": "^1.8.1",
    "fresh": "^0.5.2",
    "mime-types": "^3.0.2",
    "on-finished": "^2.4.1",
    "proxy-addr": "^2.0.7",
    "tmp-cache": "^1.1.0",
    "type-is": "^2.0.1",
    "vary": "^1.1.2"
  },
  "peerDependencies": {
    "@adonisjs/application": "^9.0.0-next.14 || ^9.0.0",
    "@adonisjs/events": "^10.1.0-next.4 || ^10.1.0",
    "@adonisjs/fold": "^11.0.0-next.4 || ^11.0.0",
    "@adonisjs/logger": "^7.1.0-next.3 || ^7.1.0",
    "@boringnode/encryption": "^0.2.0 || ^1.0.0",
    "youch": "^4.1.0-beta.13 || ^4.1.0"
  },
  "peerDependenciesMeta": {
    "youch": {
      "optional": true
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/adonisjs/http-server.git"
  },
  "bugs": {
    "url": "https://github.com/adonisjs/http-server/issues"
  },
  "homepage": "https://github.com/adonisjs/http-server#readme",
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "release-it": {
    "git": {
      "requireCleanWorkingDir": true,
      "requireUpstream": true,
      "commitMessage": "chore(release): ${version}",
      "tagAnnotation": "v${version}",
      "push": true,
      "tagName": "v${version}"
    },
    "github": {
      "release": true
    },
    "npm": {
      "publish": true,
      "skipChecks": true
    },
    "plugins": {
      "@release-it/conventional-changelog": {
        "preset": {
          "name": "angular"
        }
      }
    }
  },
  "c8": {
    "reporter": [
      "text",
      "html"
    ],
    "exclude": [
      "tests/**",
      "bin/**",
      "factories/**",
      ".yalc/**"
    ]
  },
  "prettier": "@adonisjs/prettier-config",
  "tsdown": {
    "entry": [
      "./index.ts",
      "./src/helpers.ts",
      "./src/types/main.ts",
      "./src/client/url_builder.ts",
      "./factories/main.ts"
    ],
    "noExternal": [
      "accepts",
      "negotiator"
    ],
    "inlineOnly": false,
    "outDir": "./build",
    "clean": true,
    "format": "esm",
    "minify": "dce-only",
    "fixedExtension": false,
    "dts": false,
    "treeshake": false,
    "sourcemap": false,
    "target": "esnext"
  }
}
